{
  "name": "websh",
  "version": "1.0.3",
  "description": "websh is a javascript library to compose websites.",
  "module": "websh.js",
  "main": "websh.js",
  "scripts": {
    "dev": "concurrently -k -s first \"serve -n\" \"cypress open\"",
    "build": "rollup -c rollup.config.js | terser --beautify --comments > websh.js",
    "test": "browser-sync start --startPath /tests/ --server -f tests -f src",
    "prepublishOnly": "npm run -s build"
  },
  "keywords": [
    "shell",
    "scripting",
    "compose",
    "websites"
  ],
  "files": [
    "README.org",
    "COPYING"
  ],
  "homepage": "https://github.com/domachine/websh/tree/master#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/domachine/websh.git"
  },
  "author": "Dominik Burgd√∂rfer <db@burgdoerfer.com>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "concurrently": "^4.0.1",
    "cypress": "^3.1.1",
    "rollup": "^0.67.3",
    "serve": "^10.1.1",
    "terser": "^3.10.12"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {}
}
